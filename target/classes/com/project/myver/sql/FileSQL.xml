<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"HTTP://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="file">

	<!-- 21.05.03 파일 삽입 -->
	<insert id="insertFile" parameterType="fileDTO" 
			useGeneratedKeys="true" keyProperty="file_no">
		insert into member (area, original_name, saved_name, file_size, date)
		values (#{area}, #{original_name}, #{saved_name}, #{file_size}, SYSDATE())
	</insert>
	
	<!-- 21.04.21 아이디 존재 확인
	<select id="getIDCnt" resultType="Integer" parameterType="String">
		select count(id) from member
		where id = #{id}
	</select>
	21.04.17 최근 접속일 갱신
	<update id="logDate" parameterType="String">
		update member SET log_date = SYSDATE() where id = #{id}
	</update> -->
</mapper>
	
